
{
  "expo": {
    "name": "Les Dîners Parisiens",
    "slug": "les-diners-parisiens",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/natively-dark.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/natively-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ldplehonert.ldp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/natively-dark.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.LDP.LesDinersParisiens",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/images/final_quest_240x240.png",
      "bundler": "metro",
      "output": "static",
      "name": "Les Dîners Parisiens",
      "shortName": "LDP",
      "description": "Rejoignez une communauté culinaire exclusive à Paris. Découvrez des événements gastronomiques uniques avec des chefs talentueux.",
      "themeColor": "#8B0000",
      "backgroundColor": "#000000",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "lang": "fr",
      "scope": "/",
      "preferRelatedApplications": false
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "natively",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  },
  "scheme": "les-diners-parisiens"
}
